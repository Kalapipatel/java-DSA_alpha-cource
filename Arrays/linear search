import java.util.*;

class rough3{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int arr[] = {3,5,6,7,9,4,2,1};
		int key = 7;
		int index=0;
		
		for(int i=0; i<arr.length; i++){
			if(key == arr[i]){
				index = i;
			}
		}

		System.out.println("index of the key is : "+ index);

	}
}
