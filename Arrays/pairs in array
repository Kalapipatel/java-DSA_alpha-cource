import java.util.*;

class rough3{
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int arr[] = {2,4,6,8,10};
		int l = arr.length;
		for(int i=0; i<l; i++){
			for(int j=i+1; j<l; j++){
				System.out.print("( "+ arr[i] + " , " + arr[j] + " )");
			}
			System.out.println();
		}

	}
}
