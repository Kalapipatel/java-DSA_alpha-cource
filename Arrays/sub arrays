import java.util.*;

class rough3{
	public static void main(String[] args) {
		int arr[] = {2,4,6,8,10};

		int n = arr.length;
		int total=0;

		for(int i=0; i<n; i++){
			for(int j=i; j<n; j++){
				for(int k=i; k<=j; k++){
					System.out.print(arr[k]+" ");
				}
				total++;
				System.out.println();
			}
			System.out.println();
		}
		System.out.println("number of sub arrays is : "+ total);
	}
}
