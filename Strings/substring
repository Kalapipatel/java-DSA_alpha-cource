package Strings;

public class substring {
    public static String substring(String str, int si, int ei) {
        String substring = "";

        for (int i = si; i < ei; i++) {
            substring += str.charAt(i);
        }
        return substring;
    }

    public static void main(String args[]) {

        String s = "Hello world";
        System.out.println(substring(s, 0, 5));
    }
}
